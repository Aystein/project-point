{
    "compilerOptions": {
      "target": "ESNext",
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
      "noImplicitAny": false,
      "removeComments": true,
      "preserveConstEnums": true,
      "sourceMap": true,
      "declaration": true,
      "moduleResolution": "node",
      "outDir": "./dist",
      "rootDir": ".",
      "allowSyntheticDefaultImports": true,
      "jsx": "react-jsx",
      "module": "esnext",
      "strict": false,
      "esModuleInterop": true
    },
    "include": ["src"]
  }